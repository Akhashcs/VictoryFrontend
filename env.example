# Client Environment Variables
# Copy this file to .env and fill in your actual values
# Note: All variables must be prefixed with REACT_APP_

# Backend API Configuration
REACT_APP_API_URL=https://victorybackend-7cue.onrender.com
# For development:
# REACT_APP_API_URL=http://localhost:5000

# WebSocket Configuration
REACT_APP_WS_URL=wss://victorybackend-7cue.onrender.com
# For development:
# REACT_APP_WS_URL=ws://localhost:5000

# Fyers Configuration
REACT_APP_FYERS_CLIENT_ID=your-fyers-client-id
REACT_APP_FYERS_REDIRECT_URI=https://your-frontend-domain.com/auth/fyers/callback
# For development:
# REACT_APP_FYERS_REDIRECT_URI=http://localhost:3000/auth/fyers/callback

# App Configuration
REACT_APP_APP_NAME=Victory Trading
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_LIVE_TRADING=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id
REACT_APP_SENTRY_DSN=your-sentry-dsn 